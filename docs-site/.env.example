# DungeonBreak Docs Site (Payload + Fumadocs)
# Production: Supabase Postgres + Supabase Storage (S3-compatible).
# Copy to .env and set real values locally; on Vercel use Project â†’ Environment Variables.

NEXT_PUBLIC_APP_URL=http://localhost:3000

# Payload CMS (required)
PAYLOAD_SECRET=your-secret-key-here

# Database: Supabase Postgres (production). For local SQLite use DATABASE_URL=file:./payload.db
DATABASE_URL=postgresql://user:password@host:6543/postgres

# Supabase Storage (S3-compatible) - create bucket in Dashboard, then set in .env
S3_BUCKET=your-bucket
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_REGION=us-east-2
S3_ENDPOINT=https://your-project.supabase.co/storage/v1/s3

# OpenAI (server-side; for image generation)
OPENAI_API_KEY=
OPENAI_IMAGE_MODEL=gpt-image-1

# AI / generation (optional)
AI_DEFAULT_MODEL=openai/gpt-oss-120b:free
AI_IMAGE_GENERATION_MODEL=black-forest-labs/flux.2-klein-4b
ALLOW_CLIENT_LOG=0
ELEVENLABS_API_KEY=
ELEVENLABS_TTS_MODEL_ID=eleven_multilingual_v2
NEXT_PUBLIC_LOCAL_DEV_AUTO_ADMIN=0
NEXT_PUBLIC_LOCAL_DEV_AUTO_ADMIN_EMAIL=admin@example.com
NEXT_PUBLIC_LOCAL_DEV_AUTO_ADMIN_PASSWORD=change-me
NEXT_PUBLIC_LOG_TO_SERVER=0
NEXT_PUBLIC_POSTHOG_HOST=
NEXT_PUBLIC_POSTHOG_KEY=
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL_FAST=openai/gpt-oss-120b:free
REACT_EDITOR=

# Email (Resend; optional)
RESEND_API_KEY=
RESEND_SMTP_HOST=smtp.resend.com
RESEND_SMTP_PASSWORD=
RESEND_SMTP_PORT=465
RESEND_SMTP_USER=resend

# Seed users (dev only; optional)
SEED_ADMIN_EMAIL=admin@example.com
SEED_ADMIN_PASSWORD=change-me
SEED_USER_EMAIL=user@example.com
SEED_USER_PASSWORD=change-me

# Stripe (optional)
STRIPE_ACCOUNT_ID=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
