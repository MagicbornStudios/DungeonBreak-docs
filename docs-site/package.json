{
  "name": "fumadocs-payload-template",
  "version": "1.3.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build --webpack",
    "build:vercel-parity": "cross-env CI=1 NEXT_PRIVATE_BUILD_WORKER=1 next build --webpack",
    "build:vercel-local": "pnpm dlx vercel build",
    "prebuild": "node scripts/ensure-engine-dist.mjs && node scripts/ensure-kaplay-game.mjs",
    "dev": "node scripts/ensure-kaplay-game.mjs && next dev --webpack",
    "start": "next start",
    "start:test": "next start -p 3100",
    "preinstall": "node scripts/ensure-engine-dist.mjs",
    "postinstall": "node scripts/ensure-engine-dist.mjs && node scripts/ensure-kaplay-game.mjs && fumadocs-mdx",
    "lint": "biome lint",
    "typecheck": "tsc --noEmit",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run",
    "test:e2e": "pnpm run build && playwright test",
    "test:browser-game": "pnpm run test:unit && pnpm run test:e2e",
    "report:balance-sim": "tsx scripts/report-balance-sim.ts",
    "report:vector-usage": "tsx scripts/report-vector-usage.ts",
    "space:precompute": "node scripts/space-data.mjs",
    "playwright:install": "playwright install --with-deps chromium",
    "format": "biome format app collections components lib --write",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts payload",
    "seed": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/seed-payload.ts",
    "sync:game-snapshot": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/sync-game-snapshot.ts",
    "migrate": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts payload migrate",
    "migrate:create": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts payload migrate:create",
    "push-local-to-supabase": "tsx scripts/push-local-to-supabase.ts"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.12.14",
